<h1>List of available Items in Category</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <tbody>
    <% @item_show.each do |item| %>
      <tr>
        <td><%= @item.name%></td>
        <td><%= @item.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>