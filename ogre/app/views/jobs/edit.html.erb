<h1>Edit Job</h1>

<%= form_with(model: @job, local: true) do |form| %>

 
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :img %><br>
    <%= form.text_field :img %>
  </p>

  <p>
    <%= form.label :lvl %><br>
    <%= form.text_field :lvl %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back to Job Index', jobs_path %>